pipeline{
    agent any
    tools {
        maven 'maven'
    }
    stages {
        stage('Build Maven') {
            steps{
                sh "mvn -Dmaven.test.failure.ignore=true clean package"
            }
        }
	
	 stage('Build Docker Image') {
            steps {
                script {
                  sh 'sudo docker build -t rajathegde/my-app-1.0 .'
                }
            }
        }
	}
}
